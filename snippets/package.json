{
  "name": "nvim-snippets",
  "author": "phuongthuan",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "lua",
        "path": "./lua.json"
      },
      {
        "language": [
          "shellscript",
          "shell",
          "sh",
          "zsh",
          "yml",
          "yaml"
        ],
        "path": "./shell.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "javascript.jsx",
          "typescript",
          "typescriptreact",
          "typescript.tsx"
        ],
        "path": "./typescript.json"
      },
      {
        "language": "json",
        "path": "./json.json"
      },
      {
        "language": "markdown",
        "path": "./markdown.json"
      }
    ]
  }
}
